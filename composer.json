{
    "name": "jolitagrazyte/discogs-api-wrapper",
    "description": "A simple Discogs Api wrapper using Guzzle6.",
    "keywords": [
        "Discogs Api"
    ],
    "homepage": "https://github.com/jolitagrazyte/discogs-api-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "hello@jolitagrazyte.com",
            "homepage": "http://jolitagrazyte.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "5.*",
        "guzzlehttp/guzzle":"^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Jolita\\DiscogsApiWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jolita\\DiscogsApiWrapper\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
